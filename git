#Adding only tracked files to index with git add -u and ignoring untracked files for next commit
git add -u

#Unstage files This will unstage the file but maintain the modifications.
use "git reset HEAD <file>..." to unstage.

#Revert the fiel back to the state it was before the changes
git checkout -- <file>

#view unpushed commits
git log origin/master..HEAD

#diff of unpushed commits
git diff origin/master..HEAD

#list remote branches
git branch -a

#get count of all revisions across alkl branches
git rev-list --all --count
